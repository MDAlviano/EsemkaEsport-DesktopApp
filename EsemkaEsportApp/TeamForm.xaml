<Window x:Class="EsemkaEsportApp.TeamForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EsemkaEsportApp"
        mc:Ignorable="d"
        Title="TeamForm" Height="550" Width="800" WindowStartupLocation="CenterScreen" Background="#190345">
    <Grid>
        <Button Content="Back" Click="Back_Click" Foreground="White" FontSize="16" HorizontalAlignment="Left" Height="30" Width="100" Margin="35,28,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.425,1.151" Background="#FF49376D" BorderBrush="{x:Null}" />
        <Label Content="Team" Foreground="White" FontSize="28" FontWeight="Medium" HorizontalAlignment="Center" VerticalAlignment="Top" Height="50" Margin="0,20,0,0"/>
        <DataGrid x:Name="teamDataGrid" Width="800" Height="280" Margin="40,0"/>
    </Grid>
</Window>
